{
  "expo": {
    "name": "fightder",
    "slug": "fightder",
    "scheme": "fightder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "associatedDomains": ["applinks:fightder.com"],
      "bundleIdentifier": "com.fightder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptivefync.png",
        "backgroundColor": "#161F2D"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.fightder.in",
              "pathPrefix": "/records"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.fightder"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to find users based on your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe5fbc73-81d8-4d66-b52a-3601cc5409fc"
      }
    },
    "owner": "fightder"
  }
}
